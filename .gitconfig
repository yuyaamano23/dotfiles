[alias]
        a = add
        st = status
	sh = stash
        cm = commit -v
        cmm = commit -m
        cma = commit --amend
        cmam = commit --amend -m
	ae = --allow-empty
        co = checkout
        rs = restore
        tree = log --decorate --oneline --graph
        df = diff
        dfc = diff --cached
        br = branch
