        rs = restore
	      pl = pull
        plp = pull --prune
        ps = push
        poH = push origin HEAD
	      cl = clean -fdf
        rbr = branch --sort=committerdate

[init]
        defaultBranch = main
[commit]
	gpgsign = true
[gpg]
	program = /usr/local/bin/gpg
